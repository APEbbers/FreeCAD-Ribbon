<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<package format="1" xmlns="https://wiki.freecad.org/Package_Metadata">

  <name>FreeCAD Ribbon</name>

  <description>A Ribbon for FreeCAD</description>

  <version>1.0.1</version>

  <date>2024-07-28</date>

  <maintainer>geolta</maintainer>

  <license file="LICENSE">MIT License</license>

  <pythonmin>3.8.0</pythonmin>

  <url type="website">https://github.com/geolta/FreeCAD-Ribbon/wiki</url>

  <url branch="main" type="repository">https://github.com/geolta/FreeCAD-Ribbon</url>

  <url type="readme">https://github.com/geolta/FreeCAD-Ribbon/blob/main/README.md</url>

  <icon></icon>

  <content>
    <workbench>
      <freecadmin>0.21.0</freecadmin>
      <icon>Resources/Icons/FreecadNew.svg</icon>
      <depend optional="False" type="python">pyqtribbon</depend>
      <depend optional="False" type="python">qtpy</depend>
       <!-- If this package is installed, the Addon Manager may warn the user to remove it -->
      <replace>CommandPanel</replace>
      <replace>ToolbarStyle</replace>
      <replace>PieMenu</replace>
      <subdirectory>./</subdirectory>
    </workbench>
  </content>

</package>
